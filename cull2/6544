CREATE TABLE t15(a INTEGER PRIMARY KEY, b);
INSERT INTO t15(a,b)VALUES(10,'0'),(20,'000'),(30,'000');
ALTER TABLE t15 ADD COLUMN c;
CREATE INDEX t000 ON t15(c);
