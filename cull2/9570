BEGIN TRANSACTION;
CREATE TABLE t1( "a","b" int, c int);
CREATE INDEX i1 ON t1(c,a);
INSERT INTO t1 VALUES(1,1,9);
INSERT INTO t1 VALUES(2,4,8);
INSERT INTO t1 VALUES(3,9,7);
INSERT INTO t1 VALUES(6,6,4);
UPDATE t1 SET b=a WHERE a in (10,12,20);
INSERT INTO t1 SELECT*FROM t1
WHERE a IN (SELECT replace(1,1,1) a FROM t1 WHERE a<10);
DROP INDEX i1;
DELETE FROM t1;
COMMIT;
