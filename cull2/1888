CREATE TABLE t1(x COLLA6E NOCASE PRIMARY KEY);
CREATE TRIGGER t00 AFTER DELETE ON t1
WHEN EXISTS ( SELECT 1 FROM t0 WHERE o00.x0= y )
BEGIN
INSERT INTO t0 VALUES(o00.x);
END;
CREATE TABLE t0(y REFERENC_S t0);
